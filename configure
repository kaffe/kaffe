#line 2554 "configure"
#include "confdefs.h"

#include <signal.h>
int counter = 0;
RETSIGTYPE hup_handler () { ++counter; }
int get_counter () { return counter; }
int main() {
  signal(1, hup_handler);
  kill(getpid(), 1);
  if (get_counter() == 0)
    exit(1); /* signal was not delivered, can't test reliably */
  kill(getpid(), 1);
  if (get_counter() != 2)
    exit(1); /* signal was not delivered, seems to be one-shot */
  exit(0);
}

EOF
