		check_null(PUTFIELD, stack(2), 38);
		store_offset_long(stack(2), FIELD_BOFFSET(field_field()), stack_long(0));
		pop(3);
		break;
	case 'D':
		check_stack_ref(2);
		check_null(PUTFIELD, stack(2), 39);
		store_offset_double(stack(2), FIELD_BOFFSET(field_field()), stack_double(0));
		pop(3);
		break;
