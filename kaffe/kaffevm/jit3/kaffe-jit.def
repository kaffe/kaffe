	else switch (CLASS_PRIM_SIG(FIELD_TYPE(field_field()))) {
	case 'I':
		check_stack_ref(1);
		check_null(PUTFIELD, stack(1), 36);
		store_offset_int(stack(1), FIELD_BOFFSET(field_field()), stack(0));
		pop(2);
		break;
	case 'S':
